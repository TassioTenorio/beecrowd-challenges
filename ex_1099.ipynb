    "\tfor a in range(vlr2+1, vlr1):\n",
    "\t\ta = int(a)\n",
    "\t\tif a % 2 != 0:\n",
    "\t\t\timpar += a\n",
    "\tprint(impar)\n",
    "\timpar = 0"
